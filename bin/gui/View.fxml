<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ViewController">
   <children>
      <Label layoutX="60.0" layoutY="57.0" text="Calculate Sum">
         <font>
            <Font name="DejaVu Sans Bold" size="20.0" />
         </font>
      </Label>
      <TextField fx:id="txtN1" layoutX="60.0" layoutY="144.0" promptText="Number" />
      <TextField fx:id="txtN2" layoutX="60.0" layoutY="190.0" promptText="Number" />
      <Label layoutX="60.0" layoutY="310.0" text="Result = " />
      <Label fx:id="labelResult" layoutX="128.0" layoutY="310.0" />
      <Button fx:id="btnSum" layoutX="59.0" layoutY="240.0" mnemonicParsing="false" onMouseClicked="#onBtnSumAction" text="Sum" />
      <ComboBox fx:id="comboBoxUser" layoutX="293.0" layoutY="144.0" onAction="#onComboBoxUserAction" prefWidth="150.0" promptText="Users" />
   </children>
</AnchorPane>
